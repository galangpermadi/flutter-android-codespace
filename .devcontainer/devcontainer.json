{
  "name": "Flutter Android Dev (Universal)",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "remoteUser": "codespace",
  "containerEnv": {
    "PATH": "/home/codespace/flutter/bin:/home/codespace/androidsdk/emulator:/home/codespace/androidsdk/tools:/home/codespace/androidsdk/cmdline-tools/latest/bin:/home/codespace/androidsdk/platform-tools:${PATH}"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "Dart-Code.flutter",
        "Dart-Code.dart-code",
        "esbenp.prettier-vscode",
        "ms-azuretools.vscode-docker"
      ]
    }
  }
}
